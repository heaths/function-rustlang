{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    ".cspell.json",
    ".devcontainer/**",
    ".funcignore"
  ],
  "words": [
    "clippy",
    "customhandler",
    "funcignore",
    "functionapp",
    "reqwest",
    "rustfmt",
    "rustlang"
  ],
  "overrides": [
    {
      "filename": "azure.yaml",
      "words": [
        "postrestore",
        "predeploy",
        "postup"
      ]
    },
    {
      "filename": "**/*.md",
      "dictionaries": [
        "rust"
      ]
    }
  ]
}
